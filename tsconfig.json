{
	"compilerOptions": {
		"target": "ES6",
		"module": "CommonJS",
		"experimentalDecorators": true,
		"allowJs": true,
		"allowSyntheticDefaultImports": true,
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"noImplicitAny": true,
		"jsx": "react",
		"jsxFactory": "h",
		"esModuleInterop": true,
		"downlevelIteration": true,
		"outDir": "dist/public/js",
		"lib": [
			"es2017",
			"es2019",
			"DOM",
 			"DOM.Iterable"
		 ],
	},
	"exclude": [
		"dist"
	]
}
